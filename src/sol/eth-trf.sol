pragma solidity ^0.4.0;

/// @title Transfer Ether = Eth Trf

// Declare the smart contract
contract ethTrf {

  /** @dev Can you query the balance of your account address over the Ethereum network ...
      And, then send units of wei to an alternative address? */

  // Establish the parameters for the dimensions of your function

  /// @param x; where the current value of `x` = the receiving hash
  /// @param myAddress; where the current value of `myAddress` is assigned to the parameter `this`
  /// @param myAddress; where the resultant value of `myAddress` is assigned to the parameter `y`
  /// @param z; where the current value of `z` must = boolean true

  // Declare the function transfer
  function transfer(undefined x, undefined myAddress) returns (undefined y) {

    address x = 0x65E1d7699af76717d131C3564f4378D871158Bf6;
    address myAddress = this;

    /** @dev Declare the current boolean value of `z` */
    if z is true then x.balance < 10 && myAddress.balance >= 10

    /** @dev Declare the if - then conditional statement */
    if (z is true) x.transfer(1000000);

    otherwise if z is !true then return the value of y;
  }
}